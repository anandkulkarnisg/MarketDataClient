<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>   
	<entry key="executionMode">Threaded</entry>
	<entry key="displayMode">false</entry>
	<entry key="NSEStatus">true</entry>
	<entry key="BSEStatus">true</entry>
	<entry key="cycleSleepDurationMilliSecs">10</entry>
	<entry key="maxTickLimit">1000</entry>
	<entry key="kdbServer">localhost</entry>
	<entry key="kdbPort">5000</entry>
	<entry key="KdbPublishThreads">3</entry>
	<entry key="resultWriterDestination">KDB</entry>
	<entry key="queueCapacityAdjFactor">0.5</entry>	        
</properties>